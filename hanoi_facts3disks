(disk1 Disk)
(disk2 Disk)
(disk3 Disk)
(pegA Peg)
(pegB Peg)
(pegC Peg)

(preconds
    (not-equal pegA pegB)
    (not-equal pegA pegC)
    (not-equal pegB pegA)
    (not-equal pegB pegC)
    (not-equal pegC pegB)
    (not-equal pegC pegA)

    (on disk1 pegC)
    (on disk2 pegC)
    (on disk3 pegA) 
    


)

(effects
    (on disk1 pegC)
    (on disk2 pegC)
    (on disk3 PegC) 
)